{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**",
    "dist/**",
    "test/**",
    "**/*.spec.ts"
  ],
  "reporter": [
    "text-summary",
    "html",
    "lcov"
  ],
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80
}
